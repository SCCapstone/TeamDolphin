<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.teamdolphin">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="DolphinArt"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TeamDolphin">
        <activity
            android:name=".TesterCanvas"
            android:exported="true" />
        <activity
            android:name=".CanvasDrawer"
            android:exported="true"
            android:label="@string/title_activity_canvas_drawer"
            android:theme="@style/Theme.TeamDolphin.NoActionBar" />
        <activity
            android:name=".FileCreation"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>